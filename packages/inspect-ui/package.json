{
  "name": "@effection/inspect-ui",
  "version": "2.0.0-beta.12",
  "description": "Web interface for inspecting effection applications",
  "main": "index.js",
  "types": "index.d.ts",
  "module": "dist/inspect-ui.esm.js",
  "sideEffects": [
    "*.css",
    "./index.js"
  ],
  "homepage": "https://github.com/thefrontside/effection",
  "repository": {
    "type": "git",
    "url": "https://github.com/thefrontside/effection.git",
    "directory": "packages/inspect-ui"
  },
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "prepack": "parcel build --target app ./app/index.html",
    "start": "parcel serve --target app ./app/index.html",
    "watch": "parcel watch --target app ./app/index.html",
    "lint": "eslint '{app,src,tests}/**/*.{ts,tsx}'",
    "test": "mocha -r ts-node/register --timeout 5000 test/**/*.test.{ts,tsx}",
    "docs": "echo noop",
    "mocha": "mocha -r ts-node/register --timeout 5000"
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.29.0",
    "@effection/core": "2.0.0-beta.10",
    "@effection/inspect-utils": "2.0.0-beta.12",
    "@effection/events": "2.0.0-beta.12",
    "@effection/main": "2.0.0-beta.12",
    "@effection/mocha": "2.0.0-beta.9",
    "@effection/react": "2.0.0-beta.13",
    "@effection/subscription": "2.0.0-beta.12",
    "@effection/websocket-client": "2.0.0-beta.13",
    "@frontside/tsconfig": "^1.2.0",
    "@types/jsdom-global": "^3.0.2",
    "@types/node": "^13.13.5",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect": "^25.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.3.1",
    "parcel": "^2.0.0-rc.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^8.9.0",
    "typescript": "^3.7.0"
  },
  "volta": {
    "node": "12.16.0",
    "yarn": "1.19.1"
  },
  "app": "./dist/ui/index.html",
  "targets": {
    "app": {
      "engines": {
        "browsers": "last 2 Chrome versions"
      },
      "distDir": "./dist/ui",
      "publicUrl": "/ui"
    }
  }
}
